<h1> TinyApp </h1>
<% if (username) { %>
  <h2><%=username%></h2>
    <form method="POST" action="/logout">
  <button type="submit">sign out</button>
  </form>

<% } else {%>


  <form method="POST" action="/login">
  <input type="text" name="username" placeholder="username" style="width: 300px">
  <button type="submit">sign in</button>
  </form>

  <%}%>



